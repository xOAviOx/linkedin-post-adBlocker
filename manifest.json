{
  "manifest_version": 3,
  "name": "LinkedIn PostBlocker",
  "description": "Blocking LinkedIn Posts.",
  "version": "0.0.1",
  "author": "Avi Shukla",
  "action": {
    "default_title": "LinkedIn PostBlocker"
  },
  "permissions": ["scripting", "activeTab", "tabs"],
  "host_permissions": ["https://www.linkedin.com/*"],
  "background": {
    "service_worker": "extension.js"
  }
}
