{
  "manifest_version": 2,
  "name": "LinkedIn PostBlocker",
  "description": "Blocking Posts.",
  "version": "0.0.1",
  "author": "Avi Shukla",
  "browser_action": {
      "default_title": "LinkedIn PostBlocker"
  },
  "permissions": [
      "tabs",
      "webNavigation",
      "https://www.linkedin.com/"
  ],
  "background": {
      "scripts": [
          "extension.js"
      ]
  }
}